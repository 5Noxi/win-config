        },
        "DisableAudioEnhRender": {
          "Action": "run_powershell",
          "Elevated": true,
          "Command": "Get-ChildItem 'HKLM:\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\MMDevices\\Audio\\Render' -ea SilentlyContinue | ForEach-Object { $p=Join-Path $_.PSPath 'Properties'; New-Item -Path $p -Force -ea SilentlyContinue | Out-Null; New-ItemProperty -Path $p -Name '{b3f8fa53-0004-438e-9003-51a46e139bfc},3' -PropertyType DWord -Value 0 -Force | Out-Null; New-ItemProperty -Path $p -Name '{b3f8fa53-0004-438e-9003-51a46e139bfc},4' -PropertyType DWord -Value 0 -Force | Out-Null }"
