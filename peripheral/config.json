  "Device Manager": {
    "__control": { "type": "button", "label": "Clean Up" },
    "COMMANDS": {
      "CleanupUnknownPnPDevices": {
        "Action": "run_powershell",
        "Command": "Get-PnpDevice -PresentOnly:$false | Where-Object Status -eq 'Unknown' | ForEach-Object { & pnputil /remove-device \"$($_.InstanceId)\" }"
      }
    },
    "SUBOPTION": {
      "WAN Miniports": {
        "__control": { "type": "button", "label": "Disable" },
        "COMMANDS": {
          "DisableWanMiniports": {
            "Action": "run_powershell",
            "Command": "$names=@('WAN Miniport (IKEv2)','WAN Miniport (IP)','WAN Miniport (IPv6)','WAN Miniport (L2TP)','WAN Miniport (Network Monitor)','WAN Miniport (PPPOE)','WAN Miniport (PPTP)','WAN Miniport (SSTP)'); $found=$false; foreach($n in $names){ $devs = Get-PnpDevice -FriendlyName $n -ErrorAction SilentlyContinue; if(-not $devs){ $devs = Get-PnpDevice -FriendlyName ('*{0}*' -f $n) -ErrorAction SilentlyContinue } if($devs){ $found=$true; foreach($d in $devs){ Disable-PnpDevice -InstanceId $d.InstanceId -Confirm:$false -ErrorAction SilentlyContinue } } } if(-not $found){ throw 'No matching WAN Miniport devices found.' }"
          }
        }
      },
      "High Definition Audio Controller": {
        "__control": { "type": "button", "label": "Disable" },
        "COMMANDS": {
          "DisableHDAController": {
            "Action": "run_powershell",
            "Command": "$devs = Get-PnpDevice -FriendlyName '*High Definition Audio Controller*' -ErrorAction SilentlyContinue; if (-not $devs) { throw 'No matching High Definition Audio Controller devices found.' }; foreach ($d in $devs) { Disable-PnpDevice -InstanceId $d.InstanceId -Confirm:$false -ErrorAction SilentlyContinue }"
          }
        }
      }
    }
  },
