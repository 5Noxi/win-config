  "Enable MSI Mode": {
    "apply": {
      "COMMANDS": {
        "EnableMSI": {
          "Action": "run_powershell",
          "Command": "$classes='Win32_USBController','Win32_VideoController','Win32_NetworkAdapter','Win32_IDEController'; $classes | ForEach-Object { Get-CimInstance -ClassName $_ -EA SilentlyContinue } | Where-Object { $_.PNPDeviceID -and ($_.PNPDeviceID -like 'PCI\\VEN_*') } | ForEach-Object { $base=('HKLM:\\SYSTEM\\CurrentControlSet\\Enum\\{0}\\Device Parameters\\Interrupt Management' -f $_.PNPDeviceID); $msi=Join-Path $base 'MessageSignaledInterruptProperties'; New-Item -Path $msi -Force -EA SilentlyContinue | Out-Null; New-ItemProperty -Path $msi -Name 'MSISupported' -PropertyType DWord -Value 1 -Force -EA SilentlyContinue | Out-Null; $aff=Join-Path $base 'Affinity Policy'; if(Test-Path $aff){ Remove-ItemProperty -Path $aff -Name 'DevicePriority' -EA SilentlyContinue }; Remove-ItemProperty -Path $msi -Name 'MessageNumberLimit' -EA SilentlyContinue }"
        }
      }
    },
    "revert": {
      "COMMANDS": {
        "RemoveMSI": {
          "Action": "run_powershell",
          "Command": "$classes='Win32_USBController','Win32_VideoController','Win32_NetworkAdapter','Win32_IDEController'; $classes | ForEach-Object { Get-CimInstance -ClassName $_ -EA SilentlyContinue } | Where-Object { $_.PNPDeviceID -and ($_.PNPDeviceID -like 'PCI\\VEN_*') } | ForEach-Object { $base=('HKLM:\\SYSTEM\\CurrentControlSet\\Enum\\{0}\\Device Parameters\\Interrupt Management' -f $_.PNPDeviceID); $msi=Join-Path $base 'MessageSignaledInterruptProperties'; if(Test-Path $msi){ Remove-ItemProperty -Path $msi -Name 'MSISupported' -EA SilentlyContinue; Remove-ItemProperty -Path $msi -Name 'MessageNumberLimit' -EA SilentlyContinue }; $aff=Join-Path $base 'Affinity Policy'; if(Test-Path $aff){ Remove-ItemProperty -Path $aff -Name 'DevicePriority' -EA SilentlyContinue } }"
        }
      }
    }
