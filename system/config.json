  "Remove Windows.old": {
    "__control": { "type": "button", "label": "Remove" },
    "COMMANDS": {
      "RemoveWindowsOld": {
        "Action": "elevated_priv",
        "Command": "Remove-Item -Path \"$env:SystemDrive\\Windows.old\" -Recurse -Force"
      }
    }
  },
