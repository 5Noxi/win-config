  "Notepad++": {
    "apply": {
      "COMMANDS": {
        "AddRegistryEntry": {
          "Action": "run_powershell",
          "Command": "reg add \"HKCR\\batfile\\shell\\edit\\command\" /ve /d \"\\\"C:\\Program Files\\Notepad++\\notepad++.exe\\\" \\\"%1\\\"\" /f"
        }
      }
    },
    "revert": {
      "COMMANDS": {
        "RemoveRegistryEntry": {
          "Action": "run_powershell",
          "Command": "reg delete \"HKCR\\batfile\\shell\\edit\\command\" /f"
        }
      }
    }
  },
