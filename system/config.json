  "Disable Search Indexing": {
    "apply": {
      "HKLM\\SOFTWARE\\Policies\\Microsoft\\Windows\\Windows Search": {
        "PreventIndexOnBattery": { "Type": "REG_DWORD", "Data": 1 }
      },
      "HKLM\\Software\\Microsoft\\Windows Search\\Gather\\Windows\\SystemIndex": {
        "RespectPowerModes": { "Type": "REG_DWORD", "Data": 1 }
      },
      "HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\Search\\Preferences": {
        "WholeFileSystem": { "Type": "REG_DWORD", "Data": 1 },
        "SystemFolders": { "Type": "REG_DWORD", "Data": 0 },
        "AutoWildCard": { "Type": "REG_DWORD", "Data": 1 },
        "EnableNaturalQuerySyntax": { "Type": "REG_DWORD", "Data": 0 },
        "ArchivedFiles": { "Type": "REG_DWORD", "Data": 0 }
