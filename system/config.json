  "Clean WinSxS Folder": {
    "__control": {
      "type": "button",
      "label": "Clean"
    },
    "COMMANDS": {
      "RunDISM": {
        "Action": "run_powershell",
        "Command": "dism.exe /online /Cleanup-Image /StartComponentCleanup /ResetBase"
      }
    }
  },
