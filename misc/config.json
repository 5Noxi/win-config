  "Disable VSC Telemetry": {
    "apply": {
      "COMMANDS": {
        "SetVSCodeSettings": {
          "Action": "run_powershell",
          "Command": "\"$json=gc \"$env:APPDATA\\Code\\User\\settings.json\"|ConvertFrom-Json;$nvsettings=@{\"extensions.autoCheckUpdates\"=$false;\"extensions.showRecommendationsOnlyOnDemand\"=$true;\"git.autofetch\"=$false;\"npm.fetchOnlinePackageInfo\"=$false;\"telemetry.telemetryLevel\"=0;\"telemetry.enableCrashReporter\"=$false;\"telemetry.enableTelemetry\"=$false;\"update.mode\"=\"manual\";\"update.showReleaseNotes\"=$false;\"workbench.enableExperiments\"=$false};foreach($nkey in($nvsettings.Keys|sort)){if($json.$nkey -ne$nvsettings[$nkey]){$json|Add-Member -Type NoteProperty -Name $nkey -Value $nvsettings[$nkey] -Force}};$json|ConvertTo-Json -Depth 10|sc \"$env:APPDATA\\Code\\User\\settings.json\"\""
        }
      }
    },
    "revert": {
      "COMMANDS": {
        "DeleteVSCodeSettings": {
          "Action": "run_powershell",
          "Command": "Remove-Item -Path \"$env:APPDATA\\Code\\User\\settings.json\" -Force -ea SilentlyContinue"
        }
      }
    }
  },
